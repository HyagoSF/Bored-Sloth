        app:layout_constraintTop_toBottomOf="@+id/link_name"
        app:srcCompat="@drawable/icon_link" />

    <TextView
        android:id="@+id/link_value"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:layout_marginTop="44dp"
